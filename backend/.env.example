# Angel One SmartAPI Configuration
ANGEL_ONE_API_KEY=your_angel_one_api_key
ANGEL_ONE_CLIENT_ID=your_client_id
ANGEL_ONE_PASSWORD=your_password
ANGEL_ONE_TOTP_SECRET=your_totp_secret

# MarketAux API
MARKETAUX_API_KEY=your_marketaux_api_key

# NVIDIA API Configuration (build.nvidia.com)
# Get your API key from: https://build.nvidia.com/explore/discover
NVIDIA_API_KEY=nvapi-your_nvidia_api_key_here
NVIDIA_BASE_URL=https://integrate.api.nvidia.com/v1
# Popular models: meta/llama-3.1-405b-instruct, meta/llama-3.1-70b-instruct, mistralai/mixtral-8x7b-instruct-v0.1
NVIDIA_MODEL=meta/llama-3.1-405b-instruct

# Firebase Configuration
FIREBASE_SERVICE_ACCOUNT_PATH=./serviceAccountKey.json

# FastAPI Configuration
BACKEND_URL=http://localhost:8000
FRONTEND_URL=http://localhost:3000
ENVIRONMENT=development

# Stock Monitoring Configuration
WATCHLIST_SYMBOLS=TATAMOTORS,RELIANCE,TCS,INFY
PRICE_DROP_THRESHOLD=2.0
PRICE_SPIKE_THRESHOLD=2.0
